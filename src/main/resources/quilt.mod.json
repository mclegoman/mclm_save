{
  "schema_version": 1,
  "quilt_loader": {
    "group": "com.mclegoman",
    "id": "save",
    "version": "${version}",
    "metadata": {
      "name": "Save",
      "description": "Adds saving to versions of Minecraft that don't have it.",
      "contributors": {
        "Phantazap": "Owner",
        "dannytaylor": "Lead Developer"
      },
      "license": [
        "LGPL-3.0-or-later"
      ],
      "contact": {
        "github": "https://github.com/mclegoman",
        "email": "taylor@mclegoman.com"
      },
      "icon": "assets/save/icon.png"
    },
    "entrypoints": {
      "save_init": "com.mclegoman.save.client.Save"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_loader",
        "versions": "*"
      },
      {
        "id": "minecraft",
        "versions": "*"
      }
    ]
  },
  "mixin": "save.mixins.json",
  "access_widener" : "save.accesswidener"
}